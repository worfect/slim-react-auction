{
  "name": "worfect/slim-react-auction",
  "type": "project",
  "license": "BSD-3-Clause",
  "require": {
    "php": "^8.0",
    "doctrine/migrations": "^3.3",
    "doctrine/orm": "^2.6.2",
    "finesse/swiftmailer-defaults-plugin": "^2.0",
    "friendsofphp/php-cs-fixer": "^3.3",
    "laminas/laminas-config-aggregator": "^1.6",
    "monolog/monolog": "^2.0",
    "php-di/php-di": "^6.3",
    "ptlis/conneg": "^4.0",
    "ramsey/uuid": "^4.2",
    "slim/psr7": "^1.4",
    "slim/slim": "^4.8",
    "swiftmailer/swiftmailer": "^6.2",
    "symfony/cache": "^5.2",
    "symfony/config": "^5.0",
    "symfony/console": "^5.3",
    "symfony/translation": "^5.0",
    "symfony/validator": "^5.0",
    "twig/twig": "^3.0"
  },
  "require-dev": {
    "dg/bypass-finals": "^1.3",
    "dms/phpunit-arraysubset-asserts": "^0.3.1",
    "doctrine/data-fixtures": "^1.4",
    "guzzlehttp/guzzle": "^7.4",
    "lctrs/psalm-psr-container-plugin": "^1.4",
    "overtrue/phplint": "^3.0",
    "phpunit/phpunit": "^9.5",
    "psalm/plugin-phpunit": "^0.16.1",
    "roave/security-advisories": "dev-latest",
    "vimeo/psalm": "^4.9"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {

  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "lint": "phplint",
    "php-cs-fixer": "php-cs-fixer --config=.php-cs-fixer.php --allow-risky=yes",
    "psalm": "vendor/bin/psalm",
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-html var/test/coverage",
    "app": "php bin/app.php --ansi"
  }
}
